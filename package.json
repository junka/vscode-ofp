{
  "name": "openflow",
  "displayName": "openflow",
  "description": "openflow rules highlight and visualization",
  "version": "0.0.3",
  "publisher": "junka",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/junka/vscode-ofp"
  },
  "bugs": {
    "url": "https://github.com/junka/vscode-ofp/issues"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ofp",
        "aliases": [
          "openflow",
          "ofp"
        ],
        "extensions": [
          ".ofp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ofp",
        "scopeName": "source.ofp",
        "path": "./syntaxes/ofp.tmLanguage.json"
      }
    ]
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:ofp"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "pkg": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "typescript": "^5.0.4",
    "vsce": "^2.15.0",
    "vscode": "^1.1.10"
  }
}
